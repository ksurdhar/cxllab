

<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4 box constrained">        
      <% if(user){ %>
          <h1 class="text-center">"<%= user.get('sc_username') %>"</h1>
          <div id="player"></div>
          <div class="center">
            <img alt="Like" height="100" src="/assets/up.png" width="100" data-id="<%=user.id%>" id="newLikeBtn">
            <img alt="Dislike" height="100" src="/assets/down.png" width="100" data-id="<%=user.id%>" id="newHateBtn">
          </div>
      <% } else{ %>
          <h1 class="text-center">Your feed is empty!</h1><br><br>
          <h1 class="text-center"> :) </h1><br><br>
          <div class="col-sm-offset-4">
            <button class="btn btn-lg invert" type="button" id="reset">Reset Me!</button>
          </div>
      <% } %>  
    </div>
    <div class="col-md-4"> 
      <br><br><br><br><br><br><br>
      <ul>
        <% users.each(function(user){ %>
          <li>id: <%= user.get('id') %> email: <%= user.get('email') %></li>
        <% }); %>
      </ul>
    </div>
  </div>
</div>

