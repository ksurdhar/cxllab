<div class="container">
  <div class="row">

    <div class="col-md-4 col-md-offset-4 box constrained">
        
      <% if(user){ %>
          <h1 class="text-center"><%= user.get('sc_username') %></h1>
          <div id="player"></div>
          <div class="center">
            <img alt="Twitch logo" height="100" src="/assets/up.png" width="100" data-id="<%=user.id%>" id="newLikeBtn">
            <img alt="Twitch logo" height="100" src="/assets/down.png" width="100" data-id="<%=user.id%>" id="newHateBtn">
            <!-- <button class="btn btn-sm btn-success" type="button" data-id="<%=user.id%>" id="newLikeBtn">Like</button>
            <button class="btn btn-sm btn-danger" type="button" data-id="<%=user.id%>" id="newHateBtn">Dislike</button> -->
          </div>
      <% } else{ %>
          <h1 class="text-center">Your feed is empty!</h1><br><br><br>
          <h1 class="text-center"> :) </h1>
      <% } %>  
        
    </div>

    <div class="col-md-4"> 
        <br><br><br><br><br><br><br>
        <ul>
        <% users.each(function(user){ %>
          <li>id: <%= user.get('id') %> email: <%= user.get('email') %></li>
        <% }); %>
        </ul>
    </div>

  </div>


    
</div>

