<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <h1 class="text-center">Your Account</h1><br>
      <ul>
          <li><%= user.get('email') %></li>
          <li><%= user.get('sc_username') %></li>
      </ul>
      Your Relationships
      <ul>
        <% relationships.each(function(relationship){ %>
          <li>id: <%= relationship.get('id') %> liked_user_id: <%= relationship.get('liked_user_id') %></li>
        <% }); %>
      </ul>
      Your Matches
      <ul>
        <% matches.each(function(match){ %>
          <li>id: <%= match.get('id') %> email: <%= match.get('email') %></li>
        <% }); %>
      </ul>
      <button class="btn btn-sm btn-danger" type="button" id="testing">New Content</button>

    </div>
  </div>
</div>