<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4 box">
      <h1 class="text-center">Your Account</h1><br>

      <ul class="black-text">
        <li>Email: <%= user.get('email') %></li>
        <li>Artist Name: <%= user.get('sc_username') %></li>
      </ul>

      Your Matches
      <div id="contact">
        <ul>
          <% matches.each(function(match){ %>
            <% if( relationships.findWhere({ liked_user_id: match.id, emailed: true }) ){ %>
              <div class="emailed"><li>username: <%= match.get('sc_username') %> (emailed)</li></div>
            <% } else { %>
              <div class="contactUser" data-id="<%= match.id %>"><li>username: <%= match.get('sc_username') %></li></div>
            <% } %>
          <% }); %>
        </ul>
      </div>
      
    </div>
  </div>
</div>